router bgp 106
  neighbor 3.3.3.0 remote-as 104
  neighbor 4.4.4.0 remote-as 105
  neighbor 9.9.9.4 remote-as 108
  neighbor 10.10.10.4 remote-as 109
  neighbor 3.3.3.0 route-map rm-in in
  neighbor 3.3.3.0 route-map rm-export-1 out
  neighbor 4.4.4.0 route-map rm-in in
  neighbor 4.4.4.0 route-map rm-export-1 out
  neighbor 9.9.9.4 route-map rm-in in
  neighbor 9.9.9.4 route-map rm-export-1 out
  neighbor 10.10.10.4 route-map rm-in in
  neighbor 10.10.10.4 route-map rm-export-1 out
!
ip prefix-list pl-1 permit 0.0.0.0/0 ge 0 le 32
!
ip community-list standard cl-1 permit 100:1 
!
ip as-path access-list path-1 permit (^109_ | ^108_ | ^105_ | ^104_)
!
route-map rm-in permit 10
  match ip address prefix-list pl-1
  set community additive 200:1
!
route-map rm-in permit 20
  match ip address prefix-list pl-1
  set local-preference 99
  set community additive 200:1
!
route-map rm-export-1 permit 10
  match community cl-1
  set comm-list cl-1 delete
!
